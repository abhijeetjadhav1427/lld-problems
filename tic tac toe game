class Piece{
    public PieceType pieceType;
    Piece(PieceType pieceType){
        this.pieceType = pieceType;
    }
}
class PieceX extends Piece{
    PieceX(PieceType pieceType){
        super(pieceType);
    }
}
class PieceO extends Piece{
    PieceO(PieceType pieceType){
        super(pieceType);
    }
}
class Player{
    public String name;
    public Piece piece;
    Player(String name, Piece piece){
        this.name = name;
        this.piece = piece;
    }
}
class Board{
    public Piece pieces[][];
    
    Board(int boardSize){
        pieces = new Piece[boardSize][boardSize];
    }
    public boolean addPiece(int i, int j, Piece piece){}
}
class Game{
    private Board board;
    private Deque<Player> players;

    public void initializeGame(){}
    public void addPlayer(Player player){}
    public String startGame(){}
    private boolean isWinner(int i, int j, PieceType pieceType){}
}
